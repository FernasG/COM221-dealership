{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "@users": [
        "src/users/User.ts"
      ],
      "@vehicles": [
        "src/vehicles"
      ],
      "@vehicles/*": [
        "src/vehicles/*"
      ],
      "@routes": [
        "src/routes/routes.ts"
      ],
      "@stock": [
        "src/stock/StockItem.ts"
      ],
      "@application": [
        "src/Application.ts"
      ],
      "@controllers": [
        "src/controllers"
      ],
      "@controllers/*": [
        "src/controllers/*"
      ],
      "@middleware": [
        "src/middleware/Request.middleware.ts"
      ],
      "@handlebars-helpers": [
        "public/handlebars/HandlebarsHelpers"
      ],
      "@factory": [
        "src/factory"
      ]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}